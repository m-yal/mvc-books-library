    "express-basic-auth": "^1.2.1",
    "express-session": "^1.17.3",
    "multer": "^1.4.5-lts.1",
    "mysql": "^2.18.1",
    "mysql2": "^2.3.3",
    "mysqldump": "^3.2.0",
    "node-cron": "^3.0.2",
    "session-file-store": "^1.5.0",
    "tiny-csrf": "^1.1.3",
